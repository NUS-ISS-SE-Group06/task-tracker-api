
name: dev_pull_request

on:
  workflow_dispatch:
  #pull_request:
  #  branches:
  #    - dev

jobs:
  test_build:
    runs-on: ubuntu-latest

    steps:
    - name: Checkout Source Codes
      uses: actions/checkout@v3
    - name: Set up JDK
      uses: actions/setup-java@v3
      with:
        java-version: '21'
        distribution: 'temurin'
        cache: maven
    - name: Build with Maven
      run: mvn -B clean package
      
        
